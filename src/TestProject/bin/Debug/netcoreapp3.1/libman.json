{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "filesystem",
      "library": "node_modules/bootstrap/dist/css/bootstrap.min.css",
      "destination": "wwwroot/lib/bootstrap/css/"
    },
    {
      "provider": "filesystem",
      "library": "node_modules/bootstrap/dist/js/bootstrap.min.js",
      "destination": "wwwroot/lib/bootstrap/js/"
    },
    {
      "provider": "filesystem",
      "library": "node_modules/jquery/dist/jquery.min.js",
      "destination": "wwwroot/lib/jquery/js/"
    },
    {
      "provider": "filesystem",
      "library": "node_modules/jsgrid/dist/jsgrid.min.css",
      "destination": "wwwroot/lib/jsgrid/css/"
    },
    {
      "provider": "filesystem",
      "library": "node_modules/jsgrid/dist/jsgrid.min.js",
      "destination": "wwwroot/lib/jsgrid/js/"
    }
  ]
}